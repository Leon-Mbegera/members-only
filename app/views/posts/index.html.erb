<h2>Members Posts<h2>

<% @posts.each do |post|%>
  <ul>
    <li>
      <% if user_signed_in? %>
        <h4>Author: <%=post.user.username%></h4>
      <% end %>
    </li>
    <li>
      <h4>Title: <%=post.title%></h3>
    </li>
    <li>
      <h4>Context: <%=post.body%></h4>
    </li>
</ul>
<%end%>